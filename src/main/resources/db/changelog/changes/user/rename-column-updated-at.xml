<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
   https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet id="rename-column-updated-at" author="nik pikhmanets">
        <comment>Rename column from update_at to updated_at</comment>
        <renameColumn columnDataType="TIMESTAMP"
                      newColumnName="updated_at"
                      oldColumnName="update_at"
                      tableName="users"/>
    </changeSet>
</databaseChangeLog>